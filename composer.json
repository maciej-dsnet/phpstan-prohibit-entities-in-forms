{
    "name": "dsnet/phpstan-prohibit-entities-in-forms",
    "type": "phpstan-extension",
    "authors": [
        {
            "name": "Maciej Korba≈õ",
            "email": "maciej.korbas@dsnet.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dsnet\\PhpstanProhibitEntitiesInForms\\": "src/"
        }
    },
    "extra": {
        "phpstan": {
            "includes": [
                "config/rules.neon"
            ]
        }
